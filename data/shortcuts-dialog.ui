	<object class="AdwShortcutsSection">
	<property name="title" translatable="yes">Playback Options</property>
	<child>
		<object class="AdwShortcutsItem">
		<property name="title" translatable="yes">Toggle Repeat Mode</property>
		<property name="action-name">mpd.repeat</property>
		</object>
	</child>
	<child>
		<object class="AdwShortcutsItem">
		<property name="title" translatable="yes">Toggle Random Mode</property>
		<property name="action-name">mpd.random</property>
		</object>
	</child>
	<child>
		<object class="AdwShortcutsItem">
