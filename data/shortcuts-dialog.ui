		<object class="AdwShortcutsItem">
		<property name="title" translatable="yes">Next</property>
		<property name="action-name">mpd.next</property>
		</object>
	</child>
	<child>
		<object class="AdwShortcutsItem">
		<property name="title" translatable="yes">Previous</property>
		<property name="action-name">mpd.previous</property>
		</object>
	</child>
	<child>
		<object class="AdwShortcutsItem">
		<property name="title" translatable="yes">Seek Forward</property>
		<property name="action-name">mpd.seek-forward</property>
		</object>
	</child>
	<child>
		<object class="AdwShortcutsItem">
		<property name="title" translatable="yes">Seek Backward</property>
		<property name="action-name">mpd.seek-backward</property>
