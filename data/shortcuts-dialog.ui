		<property name="action-name">mpd.enqueue</property>
		</object>
	</child>
	<child>
		<object class="AdwShortcutsItem">
		<property name="title" translatable="yes">Tidy</property>
		<property name="action-name">mpd.tidy</property>
		</object>
	</child>
	<child>
		<object class="AdwShortcutsItem">
		<property name="title" translatable="yes">Clear</property>
		<property name="action-name">mpd.clear</property>
		</object>
	</child>
	</object>
</child>
</object>
</interface>
